        "start:localhost": "NODE_ENV=production hardhat run src/main.ts --network localhost",
        "formatting:check": "prettier '**/*.{json,sol,ts,js,md}' -c",
        "formatting:write": "prettier '**/*.{json,sol,ts,js,md}' --write",
        "debug:Endpoints": "NODE_ENV=test hardhat test test/Endpoints.test.ts",
        "debug:Config": "NODE_ENV=test hardhat test test/Config.test.ts"
    },
