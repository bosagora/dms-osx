    "devDependencies": {
        "@rollup/plugin-json": "^6.0.1",
        "@rollup/plugin-typescript": "^11.1.5",
        "@typechain/ethers-v6": "^0.5.1",
        "rollup": "^2.70.1",
        "rollup-plugin-dts": "^4.2.0",
        "typechain": "^8.3.0",
        "typescript": "^5.3.3"
    },
